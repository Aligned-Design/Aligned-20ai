{
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "framework": "vite",
  "outputDirectory": "dist/spa",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/[...all].ts"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
