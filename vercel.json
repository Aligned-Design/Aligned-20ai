{
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "publicPath": "spa",
  "routes": [
    {
      "src": "/api/?(.*)",
      "dest": "/api/[...all].ts"
    },
    {
      "src": "/(.*)",
      "dest": "/spa/index.html"
    }
  ]
}
