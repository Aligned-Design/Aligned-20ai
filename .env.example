# Supabase Configuration (Required)
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# AI Provider Configuration (Required for AI features)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
AI_PROVIDER=openai

# Application Environment
NODE_ENV=development
PORT=8080
VITE_APP_URL=http://localhost:8080
VITE_API_BASE_URL=http://localhost:8080/api

# Builder.io Configuration
VITE_BUILDER_PUBLIC_KEY=your_builder_public_key
BUILDER_PRIVATE_KEY=your_builder_private_key

# Frontend Configuration
VITE_ANALYTICS_ID=your-analytics-id

# Web Crawling Configuration
CRAWL_MAX_PAGES=50
CRAWL_TIMEOUT_MS=30000
CRAWL_USER_AGENT=AlignedAIBot/1.0 (+contact: hello@aligned-by-design.com)

# Monitoring (Optional)
SENTRY_DSN=your-sentry-dsn
